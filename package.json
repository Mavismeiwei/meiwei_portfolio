{
  "name": "devfolio",
  "version": "1.0.0",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@popperjs/core": "^2.11.0",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.0",
    "bootstrap": "^5.1.3",
    "css-loader": "^6.5.1",
    "iconify": "^1.3.0",
    "jquery": "^3.6.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.5",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.9",
    "htmlnano": "^1.1.1",
    "imagemin-cli": "^7.0.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "posthtml": "^0.16.5",
    "posthtml-cli": "^0.10.0",
    "posthtml-modules": "^0.8.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "build": "run-p build:* && cp src/CNAME dist && cp -r src/webfonts dist/webfonts",
    "build:html": "posthtml -c posthtml.json",
    "build:css": "node-sass --omit-source-map-url src/styles/_all.sass dist/css/main.css",
    "build:js": "webpack --mode=production",
    "build:img": "imagemin src/assets/*.jpg --out-dir=dist/img && imagemin src/assets/*.ico --out-dir=dist/ && imagemin src/assets/*.png --out-dir=dist/img && imagemin src/assets/*.svg --out-dir=dist/img",
    "watch": "run-p serve watch:*",
    "watch:html": "onchange \"src/views\" -- npm run build:html",
    "watch:css": "onchange \"src/styles\" -- npm run build:css",
    "watch:js": "onchange \"src/js\" -- webpack --mode=development",
    "watch:img": "onchange \"src/assets\" -- npm run build:img",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "minify-all": "minify dist/css/main.css > dist/css/main.min.css"
  },
  "description": "developer portfolio",
  "main": "index.js",
  "author": "Nikita Rusetskii",
  "license": "MIT"
}
